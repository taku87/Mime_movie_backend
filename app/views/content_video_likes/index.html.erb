<p id="notice"><%= notice %></p>

<h1>Content Video Likes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @content_video_likes.each do |content_video_like| %>
      <tr>
        <td><%= link_to 'Show', content_video_like %></td>
        <td><%= link_to 'Edit', edit_content_video_like_path(content_video_like) %></td>
        <td><%= link_to 'Destroy', content_video_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content Video Like', new_content_video_like_path %>
