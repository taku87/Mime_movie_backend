<p id="notice"><%= notice %></p>

<h1>Content Video Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @content_video_comments.each do |content_video_comment| %>
      <tr>
        <td><%= link_to 'Show', content_video_comment %></td>
        <td><%= link_to 'Edit', edit_content_video_comment_path(content_video_comment) %></td>
        <td><%= link_to 'Destroy', content_video_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content Video Comment', new_content_video_comment_path %>
