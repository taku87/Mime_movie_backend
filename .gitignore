# Ignore bundler conf
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

/public/uploads

# Ignore master key for decrypting credentials and more.
/config/master.key
.env

# Ignore lambda file
lambda_FFmpeg_user_video_and_content_videol.rb
lambda_FFmpeg_user_video_and_content_video.py
lambda_video_user_to_final.rb
